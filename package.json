{
  "name": "machinepack-fs",
  "version": "12.0.1",
  "description": "Work with the local filesystem; list files, write files, etc.",
  "scripts": {
    "custom-tests": "mocha test/** -R spec",
    "lint": "node ./node_modules/eslint/bin/eslint . --max-warnings=0",
    "test": "npm run custom-tests && npm run lint"
  },
  "keywords": [
    "machines",
    "machinepack",
    "filesystem",
    "fs",
    "await"
  ],
  "author": "Mike McNeil",
  "license": "MIT",
  "dependencies": {
    "fs-extra": "0.30.0",
    "machine": "^15.0.0-12",
    "walker": "1.0.7"
  },
  "devDependencies": {
    "@sailshq/lodash": "^3.10.2",
    "eslint": "6.6.0",
    "mocha": "3.0.2",
    "test-machinepack-mocha": "^2.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mikermcneil/machinepack-fs.git"
  },
  "machinepack": {
    "friendlyName": "Filesystem",
    "machineDir": "machines",
    "machines": [
      "cp",
      "ls",
      "rmrf",
      "write",
      "write-json",
      "read",
      "read-json",
      "exists",
      "mv",
      "mkdir",
      "ensure-dir",
      "write-sync",
      "read-sync",
      "read-stream",
      "write-stream",
      "rmrf-sync"
    ],
    "testsUrl": "https://travis-ci.org/mikermcneil/machinepack-fs"
  }
}
